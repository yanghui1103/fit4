<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="logSql">
	<!-- 增加日志记录 -->
	<insert id="insert"  parameterType="logInfo">
		insert into t_loginfo(id,params,result,creator,createtime,logtype,operatefunction,resourceid,ip,url) values
	(#{id},#{params,jdbcType=VARCHAR},#{result,jdbcType=VARCHAR},#{creator,jdbcType=VARCHAR},sysdate,#{logType},#{operateFunction,jdbcType=VARCHAR},
	#{resourceId,jdbcType=VARCHAR},#{ip},#{url})
	</insert>
	<sql id="paginationSql">
		<![CDATA[ and rownum >#{start_num} and rownum <= #{end_num} ]]>   
	</sql>
</mapper>   